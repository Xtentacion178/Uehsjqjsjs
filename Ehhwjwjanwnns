Tab:AddButton({
    Name = "Give Btools",
    Description = "Gives Btools to the player's inventory (Hammer and Copy Btool)",
    Callback = function()
        local hammer = Instance.new("HopperBin")
        hammer.Name = "Hammer"
        hammer.BinType = "Hammer"
        hammer.Parent = game.Players.LocalPlayer.Backpack
        
        local copyBTool = Instance.new("HopperBin")
        copyBTool.Name = "CopyBTool"
        copyBTool.BinType = "Clone"
        copyBTool.Parent = game.Players.LocalPlayer.Backpack
    end
})
