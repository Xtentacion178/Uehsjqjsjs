-- Função para reproduzir o som de início do Hub
local function playHubSound()
    local soundEffect = Instance.new("Sound")
    soundEffect.SoundId = "rbxassetid://8569434759" -- Sound Id
    soundEffect.Volume = 0.5 -- Volume
    soundEffect.Parent = game.Workspace -- Você pode alterar o pai se preferir
    soundEffect:Play()
end

-- Função para notificação de entrada de jogadores
local function playerEntered(player)
    OrionLib:MakeNotification({
        Name = "Notificação de Entrada",
        Content = player.Name .. " Entrou no Jogo.",
        Time = 5
    })
    playHubSound() -- Reproduzir som do Hub
end

-- Função para notificação de saída de jogadores
local function playerLeft(player)
    OrionLib:MakeNotification({
        Name = "Notificação de Saída",
        Content = player.Name .. " Saiu do Jogo.",
        Time = 5
    })
    playHubSound() -- Reproduzir som do Hub
end

-- Função para notificação de reentrada de jogadores
local function playerReentered(player)
    OrionLib:MakeNotification({
        Name = "Notificação de Reentrada",
        Content = player.Name .. " Relogou no Jogo.",
        Time = 5
    })
    playHubSound() -- Reproduzir som do Hub
end

-- Conectar os eventos aos gatilhos correspondentes
game.Players.PlayerAdded:Connect(playerEntered)
game.Players.PlayerRemoving:Connect(playerLeft)
game.Players.PlayerAdded:Connect(playerReentered)
OrionLib:Init()
