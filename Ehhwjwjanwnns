local Section = Tab:AddSection({
    Name = "Others"
})
local isIllusionV20Enabled = false
local teleportSpeed = 1e-12 -- Velocidade de teleportação muito rápida
local rotateSpeed = 9999999999999 -- Velocidade de rotação muito rápida

local function toggleIllusionV20()
    isIllusionV20Enabled = not isIllusionV20Enabled
    if isIllusionV20Enabled then
        print("Ilusão V20 ativada!")
        while isIllusionV20Enabled do
            -- Teleportes para cima e para baixo em loop
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 1, 0)
            game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(teleportSpeed), {CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, -1, 0)}):Play()
            wait(teleportSpeed)

            -- Rotações em loop
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), math.rad(180), math.rad(270))
            game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(1/rotateSpeed), {CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(90), math.rad(180), math.rad(270))}):Play()
            wait(1/rotateSpeed)
        end
    else
        print("Ilusão V20 desativada!")
    end
end

Tab:AddButton({
    Name = "Ilusão V20",
    Description = "Ativa e desativa a ilusão V20",
    Callback = function()
        toggleIllusionV20()
    end
})
